      <ProjectItem ReplaceParameters="true" TargetFileName="ContentEntry.cs">ContentEntry.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="ContentObject.cs">ContentObject.cs</ProjectItem>
      <Folder Name="Data" TargetFolderName="Data">
        <ProjectItem ReplaceParameters="true" TargetFileName="Int32Range.cs">Int32Range.cs</ProjectItem>
