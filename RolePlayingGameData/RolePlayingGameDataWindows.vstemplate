<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>RolePlayingGameDataWindows</Name>
    <Description>Data types needed by the Role-Playing Game starter kit.</Description>
    <ProjectType>CSharp</ProjectType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>RolePlayingGameDataWindows</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>__TemplateIcon.ico</Icon>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="RolePlayingGameDataWindows.csproj" File="RolePlayingGameDataWindows.csproj" ReplaceParameters="true">
      <Folder Name="Animation" TargetFolderName="Animation">
        <ProjectItem ReplaceParameters="true" TargetFileName="AnimatingSprite.cs">AnimatingSprite.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Animation.cs">Animation.cs</ProjectItem>
      </Folder>
      <Folder Name="Characters" TargetFolderName="Characters">
