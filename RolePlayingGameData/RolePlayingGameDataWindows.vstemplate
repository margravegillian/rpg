        <ProjectItem ReplaceParameters="true" TargetFileName="CharacterLevelingStatistics.cs">CharacterLevelingStatistics.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="FightingCharacter.cs">FightingCharacter.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Monster.cs">Monster.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Player.cs">Player.cs</ProjectItem>
