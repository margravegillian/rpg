        <ProjectItem ReplaceParameters="true" TargetFileName="StatisticsValue.cs">StatisticsValue.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="StatisticsValueStack.cs">StatisticsValueStack.cs</ProjectItem>
      </Folder>
      <ProjectItem ReplaceParameters="true" TargetFileName="Direction.cs">Direction.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="GameStartDescription.cs">GameStartDescription.cs</ProjectItem>
      <Folder Name="Gear" TargetFolderName="Gear">
        <ProjectItem ReplaceParameters="true" TargetFileName="Armor.cs">Armor.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Equipment.cs">Equipment.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Gear.cs">Gear.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="GearDrop.cs">GearDrop.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Item.cs">Item.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Weapon.cs">Weapon.cs</ProjectItem>
      </Folder>
      <Folder Name="Map" TargetFolderName="Map">
        <ProjectItem ReplaceParameters="true" TargetFileName="Chest.cs">Chest.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="FixedCombat.cs">FixedCombat.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Inn.cs">Inn.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Map.cs">Map.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Portal.cs">Portal.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="RandomCombat.cs">RandomCombat.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Store.cs">Store.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="StoreCategory.cs">StoreCategory.cs</ProjectItem>
      </Folder>
      <ProjectItem ReplaceParameters="true" TargetFileName="MapEntry.cs">MapEntry.cs</ProjectItem>
      <Folder Name="Properties" TargetFolderName="Properties">
        <ProjectItem ReplaceParameters="true" TargetFileName="AssemblyInfo.cs">AssemblyInfo.cs</ProjectItem>
      </Folder>
      <Folder Name="Quests" TargetFolderName="Quests">
        <ProjectItem ReplaceParameters="true" TargetFileName="Quest.cs">Quest.cs</ProjectItem>
